#: ../C/sysadmin.xml:442(title)
msgid "Enabling XKB on Solaris Systems"
msgstr ""

#: ../C/sysadmin.xml:444(primary)
msgid "XKB"
msgstr ""

#: ../C/sysadmin.xml:446(para)
