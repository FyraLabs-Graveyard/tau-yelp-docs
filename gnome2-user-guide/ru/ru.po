msgstr ""

#: /tmp/doc-l10n/cvs/gnome-user-guide.HEAD/C/gosnautilus.xml:1080(title) 
msgid "Changing Backgrounds"
msgstr ""

#: /tmp/doc-l10n/cvs/gnome-user-guide.HEAD/C/gosnautilus.xml:1085(secondary) 
